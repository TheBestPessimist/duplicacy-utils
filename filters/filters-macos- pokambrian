e:(?i)^BOOTNXT$
e:(?i)^BOOTSECT\.BAK$
e:(?i)^hiberfil\.sys$
e:(?i)^pagefile\.sys$
e:(?i)^swapfile\.sys$

# ============================ system files anywhere
# ntuser.dat may contain come other things at the end, therefore don't end with "$"
e:(?i)ntuser\.dat

e:(?i)(^|/)thumbs\.db$
e:(?i)(^|/)IconCache\.db



